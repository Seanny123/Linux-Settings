{
    "typescript.check.tscVersion": false,
    "files.associations": {
        "*.css": "postcss"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.js.map": true
    },
    "julia.enableTelemetry": true,
    "files.autoSave": "afterDelay",
    "julia.enableCrashReporter": true,
    "editor.wordWrap": "on",
    "editor.mouseWheelZoom": true,
    "window.zoomLevel": 1
}
